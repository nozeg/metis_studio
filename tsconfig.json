{
	"extends": "./.svelte-kit/tsconfig.json",
	"compilerOptions": {
		"paths": {
			"$lib": [
				"src/lib"
			],
			"$lib/*": [
				"src/lib/*"
			],
			"$components": [
				"src/components"
			],
			"$components/*": [
				"src/components/*"
			],
			"$types": [
				"src/types"
			],
			"$types/*": [
				"src/types/*"
			],
			"$projets": [
				"src/projets"
			],
			"$projets/*": [
				"src/projets/*"
			],
		},
		"plugins": [
			{
				"name": "vite-plugin-iso-import"
			}
		]
	},
}